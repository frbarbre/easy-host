{
  "name": "easy-host",
  "displayName": "EasyHost",
  "description": "Deploy your project to your own server in seconds.",
  "publisher": "frbarbre",
  "icon": "./images/icon.png",
  "version": "0.0.13",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ez-deploy.run"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/frbarbre/easy-host.git"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ez-deploy.run",
        "title": "EasyHost: Open Panel",
        "category": "EasyHost",
        "icon": "$(zap)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ez-deploy",
          "title": "EasyHost",
          "icon": "$(zap)"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "dev:webview": "cd webview-ui && npm run dev",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "esbuild src/extension.ts --bundle --outdir=dist --platform=node --external:vscode",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.24.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "ignore": "^7.0.0"
  }
}
